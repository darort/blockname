{
  "manifest_version": 3,
  "name": "Block Name Red 3",
  "version": "1.0",
  "description": "Block names dynamically",
  "permissions": ["activeTab", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["injector3.js"]
    }
  ]
}
