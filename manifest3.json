{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "3.0",
    "description": "Loads content script from GitHub",
    "permissions": ["scripting"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["injector3.js"]
        }
    ]
}
